{
  "version": 2,
  "buildCommand": "pnpm vercel:build:editor",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "regions": ["sfo1"],
  "env": {
    "NEXT_PUBLIC_VERCEL_ENV": "production"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60,
      "memory": 3008
    }
  }
}
