{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./dist",
    "declaration": false,
    "declarationMap": false,
    "moduleResolution": "node",
    "module": "esnext",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "allowJs": false,
    "baseUrl": ".",
    "paths": {
      "@envisio/core": ["../core/src/index"],
      "@envisio/core/*": ["../core/src/*"],
      "@envisio/ui": ["../ui/src/index"],
      "@envisio/ui/*": ["../ui/src/*"],
      "@envisio/config": ["../config/src/index"],
      "@envisio/config/*": ["../config/src/*"],
      "@envisio/engine-cesium": ["../engine-cesium/src/index"],
      "@envisio/engine-cesium/*": ["../engine-cesium/src/*"]
    },
    "strict": false,
    "noEmit": false
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/types/**/*"],
  "exclude": [
    "node_modules",
    "dist",
    "**/*.js",
    "**/*.jsx"
  ]
}
