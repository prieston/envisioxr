{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "declaration": true,
    "declarationMap": true,
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES2020",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "types": ["node"],
    "allowJs": false,
    "baseUrl": ".",
    "paths": {
      "@envisio/core": ["../core/dist/index"],
      "@envisio/core/*": ["../core/dist/*"],
      "@envisio/ui": ["../ui/dist/index"],
      "@envisio/ui/*": ["../ui/dist/*"],
      "@envisio/config": ["../config/dist/index"],
      "@envisio/config/*": ["../config/dist/*"],
      "@envisio/engine-cesium": ["../engine-cesium/dist/index"],
      "@envisio/engine-cesium/*": ["../engine-cesium/dist/*"]
    },
    "strict": false,
    "noEmit": false
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
