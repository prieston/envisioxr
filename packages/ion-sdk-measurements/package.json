{
  "name": "@cesiumgs/ion-sdk-measurements",
  "version": "7.4.0",
  "description": "A plugin for use with CesiumJS, providing additional widgets and measurement capabilities.",
  "license": "SEE LICENSE IN sla-cesium-ion-components.pdf",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CesiumGS/cesium-analytics.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "3D",
    "webgl",
    "geospatial",
    "map",
    "globe",
    "widgets"
  ],
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "Source/**",
    "Build/**",
    "!Build/Specs/**",
    "!Build/*Dev/**",
    "README.md"
  ],
  "sideEffects": [
    "./Source/**/*.css",
    "./Specs/**/*"
  ],
  "engines": {
    "node": ">=20.19.0"
  },
  "peerDependencies": {
    "@cesium/engine": "^20.0.0",
    "@cesium/widgets": "^13.1.0"
  },
  "type": "module",
  "scripts": {
    "coverage": "gulp coverage --workspace @cesiumgs/ion-sdk-measurements",
    "test": "gulp test --production",
    "postversion": "gulp postversion --workspace @cesiumgs/ion-sdk-measurements",
    "start": "node server.js --production",
    "start-public": "node server.js --public --production",
    "test-all": "gulp test --all --production",
    "test-webgl": "gulp test --include WebGL --production",
    "test-non-webgl": "gulp test --exclude WebGL --production",
    "test-webgl-validation": "gulp test --webglValidation --production",
    "test-webgl-stub": "gulp test --webglStub --production",
    "test-release": "gulp test --release --production"
  },
  "homepage": "https://cesium.com/platform/cesiumjs/ion-sdk/",
  "author": {
    "name": "Cesium GS, Inc.",
    "url": "https://cesium.com"
  },
  "bugs": {
    "url": "https://github.com/CesiumGS/cesium/issues"
  },
  "devDependencies": {
    "@types/three": "^0.170.0",
    "@types/uuid": "^9.0.8"
  }
}