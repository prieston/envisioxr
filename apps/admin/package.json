{
  "name": "@klorad/admin",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20 <23",
    "pnpm": ">=9"
  },
  "scripts": {
    "lint": "next lint --fix --dir app",
    "dev": "NODE_ENV=development NEXT_TELEMETRY_DISABLED=1 next dev -p 3002",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true next build",
    "size": "size-limit --why || (echo 'Bundle over budget' && exit 1)",
    "start": "next start",
    "vercel:build:admin": "cd ../.. && pnpm --filter @klorad/prisma generate && pnpm --filter @klorad/prisma prisma migrate deploy && pnpm --filter './packages/*' --workspace-concurrency=1 run build && cd apps/admin && pnpm build"
  },
  "dependencies": {
    "@klorad/prisma": "workspace:*",
    "@klorad/ui": "workspace:^",
    "@mui/icons-material": "^6.4.2",
    "@mui/material": "^6.4.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/bundle-analyzer": "^14.0.4",
    "@prisma/client": "^5.22.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.16",
    "bcryptjs": "^3.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.18",
    "eslint-config-prettier": "^9.1.0",
    "next": "^14.2.18",
    "next-auth": "4.24.6",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-toastify": "^9.1.3",
    "resend": "^6.5.0",
    "@react-email/components": "^1.0.1",
    "swr": "^2.2.4",
    "tailwindcss": "^3.4.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "cross-env": "^10.1.0",
    "size-limit": "^11.2.0",
    "typescript": "^5.9.0"
  }
}

