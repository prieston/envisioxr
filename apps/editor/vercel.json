{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm vercel:build:editor",
  "installCommand": "cd ../.. && pnpm install --frozen-lockfile",
  "framework": "nextjs",
  "regions": ["sfo1"],
  "env": {
    "NEXT_PUBLIC_VERCEL_ENV": "production",
    "SKIP_ENV_VALIDATION": "1"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "outputDirectory": ".next"
}
